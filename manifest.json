{
    "manifest_version": 3,
    "name": "Compras IA",
    "version": "1.0.5",
    "action": {
        "default_title": "Abrir Compras IAA"
    },
    "background": {
        "service_worker": "background.js",
        "type": "module"
    },
    "permissions": [
        "scripting",
        "storage",
        "activeTab"
    ],
    "icons": {
        "32": "icon32.png"
    },
    "web_accessible_resources": [
        {
            "resources": [
                "index.html",
                "main.js",
                "index.css"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}